{
	"parser": "babel-eslint",
	"extends": [
		"react-app",
		"prettier",
		"prettier/babel",
		"prettier/react"
	],
	"plugins": [
		"prettier"
	],
	"rules": {
		"react/react-in-jsx-scope": 0,
		"react/style-prop-object": 0,
		"prettier/prettier": [
			"warn",
			{
				"trailingComma": "es5",
				"semi": true,
				"singleQuote": true,
				"printWidth": 100,
				"useTabs": true,
				"bracketSpacing": true,
				"parser": "babel"
			}
		]
	}
}